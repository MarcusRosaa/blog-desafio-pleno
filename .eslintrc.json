{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "import/prefer-default-export": "off",
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "max-len": "off",
      "jsx-a11y/label-has-associated-control": "off",
      "react/jsx-no-bind": "off"
    }
}
